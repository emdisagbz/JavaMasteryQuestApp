@{
}
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Ethereal Eclave</title>
<link rel="stylesheet" href="~/css/custom/EtherealEclaveStyle.css">

<div class="container">
    <div class="bubbles"></div>
</div>

<button id="backButton">Ethereal Eclave</button>

<div class="coins-container">
    <div class="coin">
        <img src="~/image/Custom/gold.png" alt="Gold Coin" class="coin-image">
        <div class="coin-count" id="goldCountContainer">
            <span id="goldCount">0</span>
        </div>
    </div>
    <div class="coin">
        <img src="~/image/Custom/silver.png" alt="Silver Coin" class="coin-image">
        <div class="coin-count" id="silverCountContainer">
            <span id="silverCount">0</span>
        </div>
    </div>
</div>

<script src="~/js/custom/EtherealEclaveScript.js"></script>